{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "types": ["@emotion/react", "@emotion/native", "react-native"],
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@context/*": ["context/*"],
      "@hooks/*": ["hooks/*"],
      "@navigation/*": ["navigation/*"],
      "@redux/*": ["redux/*"],
      "@services/*": ["services/*"],
      "@styles/*": ["styles/*"],
      "types/*": ["./@types/*"],
      "@utils/*": ["utils/*"]
    }
  }
}
